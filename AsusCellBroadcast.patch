+            } else {
+                if (DBG) log("disabling cell broadcast channel 50");
+                manager.disableCellBroadcast(50, SmsManager.CELL_BROADCAST_RAN_TYPE_GSM);
+            }
+
+            if ("il".equals(tm.getSimCountryIso()) || "il".equals(tm.getNetworkCountryIso())) {
